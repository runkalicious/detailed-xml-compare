{
  "manifest_version": 2,
  "name": "Veracode Detailed XML Compare",
  "version": "0.9",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "assets/Monster3_16BW.png",
    "128": "assets/Monster3_128BW.png"
  },
  "app": {
    "background": {
      "scripts": [
        "main.js"
      ]
    }
  },
  "permissions": [
    "fileSystem"
  ],
  "file_handlers": {
    "text": {
      "types": [
        "text/xml"
      ]
    }
  }
}
